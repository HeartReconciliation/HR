    <div id="content" class="content">
        <!-- begin container -->
        <div class="container">
            <!-- begin post-list -->
            <div class="post-list post-grid post-grid-3">
                <% @blogs.each do |b| %>
                <br>
                <div class="post-li">
                    <!-- begin post-content -->
                    <div class="post-content">
                        <!-- begin post-image -->
                        <div class="post-image">
													<%= link_to blog_path(b) do %>
													<%= image_tag b.avatar.url %>
													<% end %>
                        </div>
                        <!-- end post-image -->
                        <!-- begin post-info -->
                        <div class="post-info">
                            <h4 class="post-title">
                                <%= link_to blog_path(b) do %>
																<%= truncate(b.title,:lenght => 12) %>
																<% end %>
                            </h4>
                            <div class="post-by">
                                Posted By <a href="post_without_sidebar.html#">admin</a> <span class="divider">|</span><%= b.created_at.strftime("%A %b %d") %>
                            </div>
                            <div class="post-desc">
                              <%= truncate(b.description,:length => 100)%>
                            </div>
                            <div class="read-btn-container">
															<%= link_to blog_path(b) do %>
															Read More <i class="fa fa-angle-double-right"></i>
															<% end %>
                            </div>
                        </div>
                        <!-- end post-info -->
                    </div>
                    <!-- end post-content -->
                </div>
                <br>
                <% end %>
            </div>
            <!-- end post-list -->

            <div class="section-container">
                <!-- begin pagination -->
                <div class="pagination-container text-center">
                    <%= will_paginate %>
                </div>
                <!-- end pagination -->
            </div>
        </div>
        <!-- end container -->
    </div>
